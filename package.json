{
  "name": "express-rest-api",
  "version": "1.0.0",
  "description": "Express RESTful APIs",
  "author": "Sastra Nababan <sastranababan@yahoo.com>",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=10",
    "npm": "*"
  },
  "scripts": {
    "precommit": "npm lint",
    "prestart": "npm docs",
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js",
